{
  "name": "khaway/laravel-taxonomy",
  "description": "Laravel taxonomy package.",
  "keywords": [
    "taxonomy",
    "laravel-taxonomy"
  ],
  "require": {
    "php": "^7.2",
    "kalnoy/nestedset": "^5.0",
    "ankurk91/laravel-eloquent-relationships": "^1.1",
    "nanigans/single-table-inheritance": "0.8.*",
    "spatie/laravel-schemaless-attributes": "^1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^3.8|^4.0",
    "phpunit/phpunit": "^9",
    "spatie/phpunit-watcher": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "Scrapify\\LaravelTaxonomy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scrapify\\LaravelTaxonomy\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scrapify\\LaravelTaxonomy\\TaxonomyServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
